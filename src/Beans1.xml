<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.springframework.org/schema/beans

    http://www.springframework.org/schema/beans/spring-beans-3.0.xsd">
   
   <bean name="javaCollection" class="com.rock1.JavaCollection">
   		<property name="addressList">
   			<list>
   				<value>aaaaa</value>
   				<value>bbbbb</value>
   				<value>ccccc</value>
   				<value>ccccc</value>
   			</list>
   		
   		</property>
   		<property name="addressSet">
   			<set>
   				<value>aaaaa</value>
   				<value>bbbbb</value>
   				<value>ccccc</value>
   				<value>ccccc</value>
   			
   			</set>
   		
   		
   		
   		</property>
   		<property name="addressMap">
   		
   			<map>
   				<entry key="1" value="aaaaa">
   				</entry>
   				<entry key="2" value="bbbbb"></entry>
   				<entry key="3" value="ccccc"></entry>
   				<entry key="3" value="ccccc"></entry>
   			
   			</map>
   		
   		
   		
   		</property>
   		</bean>
   
</beans>